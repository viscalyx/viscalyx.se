{
  "name": "viscalyx.se",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "npm run build:blog && npm run build:page-dates && next dev",
    "build": "npm run build:blog && npm run build:page-dates && cross-env NODE_ENV=production next build",
    "build:blog": "node scripts/build-blog-data.js",
    "build:page-dates": "node scripts/build-page-dates.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "spell": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "spell:check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --no-progress",
    "spell:fix": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --show-suggestions",
    "analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "test:security": "node scripts/security-audit.js",
    "check": "npm run type-check && npm run format:check && npm run spell:check && npm run lint && npm run test && npm run test:security",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "prebuild": "rimraf .next out",
    "prepreview": "rimraf .open-next"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.3.0",
    "@tailwindcss/typography": "^0.5.16",
    "framer-motion": "^12.15.0",
    "lucide-react": "^0.511.0",
    "next": "^15.3.4",
    "next-intl": "^4.1.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-prism-plus": "^2.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-rehype": "^11.1.2",
    "sanitize-html": "^2.17.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@cspell/dict-sv": "^2.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.1.6",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cspell": "^9.0.2",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.4",
    "gray-matter": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-junit": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "postcss": "^8.5.4",
    "prettier": "^3.6.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "wrangler": "^4.20.3"
  }
}
