{
  "name": "viscalyx.se",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "predev": "npm run build:sitedata",
    "dev": "next dev",
    "prebuild": "npm run clean && npm run build:sitedata",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "lint": "npx eslint . --format=stylish && next lint",
    "lint:fix": "npx eslint . --fix && next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "spell": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "spell:check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --no-progress",
    "spell:fix": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --show-suggestions",
    "clean": "rimraf .next out .open-next",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "CODECOV_TOKEN=${CODECOV_TOKEN} npm run build",
    "export": "next export",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --api.host=0.0.0.0 --api.port=51204",
    "test:security": "node scripts/security-audit.js",
    "check": "npm run type-check && npm run format:check && npm run spell:check && npm run lint && npm run test && npm run test:security",
    "prepreview": "npm run clean && npm run build:sitedata",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "build:sitedata": "npm run build:blog && npm run build:page-dates",
    "build:blog": "node scripts/build-blog-data.js",
    "build:page-dates": "node scripts/build-page-dates.js"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.6.1",
    "@tailwindcss/typography": "^0.5.16",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-intl": "^4.3.4",
    "node-html-parser": "^7.0.1",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-prism-plus": "^2.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-rehype": "^11.1.2",
    "sanitize-html": "^2.17.0",
    "slugify": "^1.6.6",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@codecov/nextjs-webpack-plugin": "^1.9.1",
    "@cspell/dict-sv": "^2.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cspell": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.2",
    "globals": "^16.3.0",
    "gray-matter": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "mermaid": "11.9.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.26.0"
  }
}
