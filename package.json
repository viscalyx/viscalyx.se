{
  "name": "viscalyx.se",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "npm run build:blog && npm run build:page-dates && next dev",
    "build": "npm run build:blog && npm run build:page-dates && cross-env NODE_ENV=production next build",
    "build:blog": "node scripts/build-blog-data.js",
    "build:page-dates": "node scripts/build-page-dates.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "spell": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "spell:check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --no-progress",
    "spell:fix": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --show-suggestions",
    "clean": "rm -rf .next out",
    "analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "check": "npm run type-check && npm run format:check && npm run spell:check && npm run lint",
    "preview": "npm run build:blog && npm run build:page-dates && opennextjs-cloudflare build && opennextjs-cloudflare preview -- --host 0.0.0.0 --port 8787 --ip $(hostname -I | awk '{print $1}')",
    "deploy": "npm run build:blog && npm run build:page-dates && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.1.0",
    "framer-motion": "^12.15.0",
    "lucide-react": "^0.511.0",
    "next": "^15.3.3",
    "next-intl": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@cspell/dict-sv": "^2.3.0",
    "@types/node": "^22.15.29",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cspell": "^9.0.2",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "gray-matter": "^4.0.3",
    "jsonc-eslint-parser": "^2.4.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "wrangler": "^4.18.0"
  }
}
