{
  "name": "viscalyx.se",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "predev": "npm run build:sitedata",
    "dev": "next dev",
    "prebuild": "node ./scripts/prebuild.js",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "lint": "npx eslint . --format=stylish",
    "lint:fix": "npx eslint . --fix",
    "type-check": "rimraf .next/dev/types && next typegen && tsc --noEmit",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "spell": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "spell:check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --no-progress",
    "spell:fix": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --show-suggestions",
    "clean:all": "rimraf .next out .open-next .wrangler",
    "analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --api.host=0.0.0.0 --api.port=51204",
    "test:security": "node scripts/security-audit.js",
    "test:integration": "playwright test",
    "test:integration:preview": "playwright test --config=playwright.preview.config.ts",
    "test:integration:ui": "playwright test --ui",
    "test:integration:report:dev": "playwright show-report --host 0.0.0.0 playwright-report-dev",
    "test:integration:report:preview": "playwright show-report --host 0.0.0.0 playwright-report-preview",
    "test:integration:all": "npm run test:integration && npm run test:integration:preview",
    "check": "npm run type-check && npm run format:check && npm run spell:check && npm run lint && npm run test && npm run test:security",
    "prepreview": "rimraf .open-next .wrangler && npm run cf-typegen",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "build:sitedata": "npm run build:blog && npm run build:page-dates",
    "build:blog": "node scripts/build-blog-data.js",
    "build:page-dates": "node scripts/build-page-dates.js",
    "purge:install": "rimraf node_modules package-lock.json && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.16.3",
    "@tailwindcss/typography": "^0.5.19",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-intl": "^4.8.2",
    "node-html-parser": "^7.0.2",
    "prismjs": "^1.30.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rehype-prism-plus": "^2.0.2",
    "rehype-stringify": "^10.0.1",
    "remark-rehype": "^11.1.2",
    "sanitize-html": "^2.17.0",
    "slugify": "^1.6.6",
    "unist-util-visit": "^5.1.0"
  },
  "devDependencies": {
    "@cspell/dict-sv": "^2.3.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.12",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "cross-env": "^10.1.0",
    "cspell": "^9.6.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "globals": "^17.3.0",
    "gray-matter": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^28.0.0",
    "jsonc-eslint-parser": "^2.4.2",
    "mermaid": "^11.12.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  },
  "overrides": {
    "@swc/helpers": "0.5.18",
    "qs": "6.14.2"
  }
}
