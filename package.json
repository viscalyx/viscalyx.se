  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "spell": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "spell:check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --no-progress",
    "spell:fix": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --show-suggestions",
    "clean": "rm -rf .next out",
    "analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "check": "npm run type-check && npm run format:check && npm run spell:check && npm run lint"
  },
  "dependencies": {
    "framer-motion": "^12.14.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "next-intl": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@cspell/dict-sv": "^2.3.0",
    "@types/node": "^22.15.21",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
